<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.pm.model.JpmSendConsignment"
            table="JPM_SEND_CONSIGNMENT"
    >

    <id
        name="consignmentNo"
            column="CONSIGNMENT_NO"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="native">

    </generator>

    </id>

        <property
            name="fabId"
                    column="FAB_ID"
                length="10"
        >

        </property>

        <property
            name="sendDate"
                    column="SEND_DATE"
                length="7"
        >

        </property>

        <property
            name="status"
                    column="STATUS"
                length="1"
        >

        </property>

        <property
            name="specNo"
                    column="SPEC_NO"
                length="10"
        >

        </property>

        <property
            name="consignmenNum"
                    column="CONSIGNMEN_NUM"
                length="22"
        >

        </property>

        <property
            name="sendUserCode"
                    column="SEND_USER_CODE"
                length="30"
        >

        </property>

    </class>    

</hibernate-mapping>


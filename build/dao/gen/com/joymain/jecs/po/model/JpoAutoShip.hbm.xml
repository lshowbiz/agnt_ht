<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.po.model.JpoAutoShip"
            table="JPO_AUTO_SHIP"
    >

    <id
        name="jasId"
            column="JAS_ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="native">

    </generator>

    </id>

        <property
            name="companyCode"
                    column="COMPANY_CODE"
                length="2"
                not-null="true"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="30"
                not-null="true"
        >

        </property>

        <property
            name="jasActionTime"
                    column="JAS_ACTION_TIME"
                length="7"
                not-null="true"
        >

        </property>

        <property
            name="moId"
                    column="MO_ID"
                length="10"
                not-null="true"
        >

        </property>

        <property
            name="createUser"
                    column="CREATE_USER"
                length="30"
                not-null="true"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
                not-null="true"
        >

        </property>

        <property
            name="status"
                    column="STATUS"
                length="2"
                not-null="true"
        >

        </property>

        <property
            name="checkTime"
                    column="CHECK_TIME"
                length="7"
        >

        </property>

        <property
            name="remark"
                    column="REMARK"
                length="3500"
        >

        </property>

        <property
            name="firstName"
                    column="FIRST_NAME"
                length="100"
                not-null="true"
        >

        </property>

        <property
            name="lastName"
                    column="LAST_NAME"
                length="100"
                not-null="true"
        >

        </property>

        <property
            name="cardCvv"
                    column="CARD_CVV"
                length="50"
                not-null="true"
        >

        </property>

        <property
            name="cardNumber"
                    column="CARD_NUMBER"
                length="50"
                not-null="true"
        >

        </property>

        <property
            name="expireDate"
                    column="EXPIRE_DATE"
                length="20"
                not-null="true"
        >

        </property>

        <property
            name="payCause"
                    column="PAY_CAUSE"
                length="4"
                not-null="true"
        >

        </property>

        <property
            name="returnData"
                    column="RETURN_DATA"
                length="3500"
        >

        </property>

    </class>    

</hibernate-mapping>


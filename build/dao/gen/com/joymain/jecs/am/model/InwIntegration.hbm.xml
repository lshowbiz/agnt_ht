<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.am.model.InwIntegration"
            table="INW_INTEGRATION"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_AM]]>
        </param>

    </generator>

    </id>

        <property
            name="suggestionUserCode"
                    column="SUGGESTION_USER_CODE"
                length="20"
        >

        </property>

        <property
            name="integrationAdd"
                    column="INTEGRATION_ADD"
                length="5"
        >

        </property>

        <property
            name="integrationAddTime"
                    column="INTEGRATION_ADD_TIME"
                length="7"
        >

        </property>

        <property
            name="suggestionid"
                    column="SUGGESTIONID"
                length="13"
        >

        </property>

        <property
            name="integrationMinus"
                    column="INTEGRATION_MINUS"
                length="5"
        >

        </property>

        <property
            name="integrationActivity"
                    column="INTEGRATION_ACTIVITY"
                length="13"
        >

        </property>

        <property
            name="integrationMinusTime"
                    column="INTEGRATION_MINUS_TIME"
                length="7"
        >

        </property>

        <property
            name="operatorUserCode"
                    column="OPERATOR_USER_CODE"
                length="20"
        >

        </property>

        <property
            name="other"
                    column="OTHER"
                length="100"
        >

        </property>

    </class>    

</hibernate-mapping>


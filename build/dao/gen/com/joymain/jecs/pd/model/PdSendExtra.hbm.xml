<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.pd.model.PdSendExtra"
            table="PD_SEND_EXTRA"
    >

    <id
        name="uniId"
            column="UNI_ID"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_PD]]>
        </param>

    </generator>

    </id>

        <property
            name="siNo"
                    column="SI_NO"
                length="17"
        >

        </property>

        <property
            name="productNo"
                    column="PRODUCT_NO"
                length="20"
        >

        </property>

        <property
            name="qty"
                    column="QTY"
                length="10"
        >

        </property>

        <property
            name="operaterCode"
                    column="OPERATER_CODE"
                length="20"
        >

        </property>

        <property
            name="exType"
                    column="EX_TYPE"
                length="5"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="remark"
                    column="REMARK"
                length="1024"
        >

        </property>

    </class>    

</hibernate-mapping>


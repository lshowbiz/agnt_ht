<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.fi.model.JfiSunDistribution"
            table="JFI_SUN_DISTRIBUTION"
    >

    <id
        name="userCode"
            column="USER_CODE"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="native">

    </generator>

    </id>

        <property
            name="createNo"
                    column="CREATE_NO"
                length="20"
                not-null="true"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
                not-null="true"
        >

        </property>

        <property
            name="userName"
                    column="USER_NAME"
                length="200"
        >

        </property>

    </class>    

</hibernate-mapping>


                    column="PASSWORD"
                length="1024"
        >

        </property>

        <property
            name="password2"
                    column="PASSWORD2"
                length="1024"
        >

        </property>

        <property
            name="busicode"
                    column="BUSICODE"
                length="256"
        >

        </property>

        <property

        </property>

        <property
            name="createName"
                    column="CREATE_NAME"
                length="256"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="256"
        >

        </property>

        <property
            name="operateName"
                    column="OPERATE_NAME"
                length="256"
        >

        </property>

        <property
            name="operateTime"
                    column="OPERATE_TIME"
                length="256"
        >

        </property>

    </class>    

</hibernate-mapping>


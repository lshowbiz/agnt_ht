<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.fi.model.FiSecurityDepositJournal"
            table="FI_SECURITY_DEPOSIT_JOURNAL"
    >

    <id
        name="journalId"
            column="JOURNAL_ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="native">

    </generator>

    </id>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
                not-null="true"
        >

        </property>

        <property
            name="serial"
                    column="SERIAL"
                length="8"
        >

        </property>

        <property
            name="dealType"
                    column="DEAL_TYPE"
                length="1"
        >

        </property>

        <property
            name="dealDate"
                    column="DEAL_DATE"
                length="7"
        >

        </property>

        <property
            name="amount"
                    column="AMOUNT"
                length="18"
        >

        </property>

        <property
            name="notes"
                    column="NOTES"
                length="4000"
        >

        </property>

        <property
            name="balance"
                    column="BALANCE"
                length="18"
        >

        </property>

        <property
            name="remark"
                    column="REMARK"
                length="4000"
        >

        </property>

        <property
            name="createrCode"
                    column="CREATER_CODE"
                length="20"
        >

        </property>

        <property
            name="createrName"
                    column="CREATER_NAME"
                length="80"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="moneyType"
                    column="MONEY_TYPE"
                length="4"
        >

        </property>

        <property
            name="uniqueCode"
                    column="UNIQUE_CODE"
                length="200"
        >

        </property>

        <property
            name="appId"
                    column="APP_ID"
                length="12"
        >

        </property>

    </class>    

</hibernate-mapping>


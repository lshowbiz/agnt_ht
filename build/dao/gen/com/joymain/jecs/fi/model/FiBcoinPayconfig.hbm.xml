<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.fi.model.FiBcoinPayconfig"
            table="FI_BCOIN_PAYCONFIG"
    >

    <id
        name="configId"
            column="CONFIG_ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_BANKBOOK]]>
        </param>

    </generator>

    </id>

        <property
            name="startTime"
                    column="START_TIME"
                length="20"
                not-null="true"
        >

        </property>

        <property
            name="endTime"
                    column="END_TIME"
                length="20"
                not-null="true"
        >

        </property>

        <property
            name="title"
                    column="TITLE"
                length="100"
                not-null="true"
        >

        </property>

        <property
            name="limit"
                    column="LIMIT"
                length="1"
                not-null="true"
        >

        </property>

        <property
            name="createCode"
                    column="CREATE_CODE"
                length="20"
        >

        </property>

        <property
            name="createName"
                    column="CREATE_NAME"
                length="20"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="proportion"
                    column="PROPORTION"
                length="1"
                not-null="true"
        >

        </property>

    </class>    

</hibernate-mapping>


        </property>

        <property
            name="inc"
                    column="INC"
                length="2"
        >

        </property>

        <property
            name="returnMsg"
                    column="RETURN_MSG"
                length="50"
        >

        </property>

        <property
            name="url"
                    column="URL"
                length="1000"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

    </class>    

</hibernate-mapping>


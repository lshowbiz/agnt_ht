        </property>

        <property
            name="orderDate"
                    column="ORDER_DATE"
                length="50"
        >

        </property>

        <property
            name="orderAmount"
                    column="ORDER_AMOUNT"
                length="50"
        >

        </property>

        <property
            name="orderTime"
                    column="ORDER_TIME"
                length="50"
        >

        </property>

        <property
            name="detailId"
                    column="DETAIL_ID"
                length="50"
        >

        </property>

        <property
            name="detailTime"
                    column="DETAIL_TIME"
                length="50"
        >

        </property>

        <property
            name="bankId"
                    column="BANK_ID"
                length="50"
        >

        </property>

        <property
            name="bankDealId"

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.fi.model.JfiTenpayLog"
            table="JFI_TENPAY_LOG"
    >

    <id
        name="logId"
            column="LOG_ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_FI]]>
        </param>

    </generator>

    </id>

        <property
            name="companyCode"
                    column="COMPANY_CODE"
                length="2"
                not-null="true"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
                not-null="true"
        >

        </property>

        <property
            name="cmdno"
                    column="CMDNO"
                length="500"
        >

        </property>

        <property
            name="payResult"
                    column="PAY_RESULT"
                length="500"
        >

        </property>

        <property
            name="payInfo"
                    column="PAY_INFO"
                length="500"
        >

        </property>

        <property
            name="tpDate"
                    column="TP_DATE"
                length="500"
        >

        </property>

        <property
            name="purchaserId"
                    column="PURCHASER_ID"
                length="500"
        >

        </property>

        <property
            name="bargainorId"
                    column="BARGAINOR_ID"
                length="500"
        >

        </property>

        <property
            name="transactionId"
                    column="TRANSACTION_ID"
                length="500"
        >

        </property>

        <property
            name="spBillno"
                    column="SP_BILLNO"
                length="500"
        >

        </property>

        <property
            name="totalFee"
                    column="TOTAL_FEE"
                length="500"
        >

        </property>

        <property
            name="feeType"
                    column="FEE_TYPE"
                length="500"
        >

        </property>

        <property
            name="attach"
                    column="ATTACH"
                length="500"
        >

        </property>

        <property
            name="sign"
                    column="SIGN"
                length="500"
        >

        </property>

        <property
            name="busType"
                    column="BUS_TYPE"
                length="500"
        >

        </property>

        <property
            name="busArgs"
                    column="BUS_ARGS"
                length="500"
        >

        </property>

        <property
            name="version"
                    column="VERSION"
                length="500"
        >

        </property>

        <property
            name="inc"
                    column="INC"
                length="1"
                not-null="true"
        >

        </property>

        <property
            name="referer"
                    column="REFERER"
                length="4000"
        >

        </property>

        <property
            name="returnMsg"
                    column="RETURN_MSG"
                length="2"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="ip"
                    column="IP"
                length="4000"
        >

        </property>

        <property
            name="url"
                    column="URL"
                length="4000"
        >

        </property>

    </class>    

</hibernate-mapping>


<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.fi.model.JfiChinapayPosLog"
            table="JFI_CHINAPAY_POS_LOG"
    >

    <id
        name="logId"
            column="LOG_ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_FI]]>
        </param>

    </generator>

    </id>

        <property
            name="company"
                    column="COMPANY"
                length="2"
        >

        </property>

        <property
            name="payDealCode"
                    column="PAY_DEAL_CODE"
                length="100"
                not-null="true"
        >

        </property>

        <property
            name="payAmount"
                    column="PAY_AMOUNT"
                length="18"
                not-null="true"
        >

        </property>

        <property
            name="fee"
                    column="FEE"
                length="18"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="30"
        >

        </property>

        <property
            name="mobileNumber"
                    column="MOBILE_NUMBER"
                length="30"
        >

        </property>

        <property
            name="ext2"
                    column="EXT2"
                length="1000"
        >

        </property>

        <property
            name="payResult"
                    column="PAY_RESULT"
                length="30"
        >

        </property>

        <property
            name="inc"
                    column="INC"
                length="1"
        >

        </property>

        <property
            name="returnMsg"
                    column="RETURN_MSG"
                length="2"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="errCode"
                    column="ERR_CODE"
                length="30"
        >

        </property>

        <property
            name="companyCode"
                    column="COMPANY_CODE"
                length="2"
        >

        </property>

        <property
            name="transCard"
                    column="TRANSCARD"
                length="100"
        >

        </property>

        <property
            name="terminalNo"
                    column="TERMINALNO"
                length="100"
        >

        </property>

        <property
            name="transRetrievalNo"
                    column="TRANS_RETRIEVAL_NO"
                length="100"
        >

        </property>

        <property
            name="transSerialNo"
                    column="TRANS_SERIA_NO"
                length="100"
        >

        </property>

        <property
            name="msgCode"
                    column="MSG_CODE"
                length="100"
        >

        </property>

        <property
            name="merchantNo"
                    column="MERCHANT_NO"
                length="100"
        >

        </property>

        <property
            name="transTime"
                    column="TRANS_TIME"
                length="7"
        >

        </property>

        <property
            name="isBack"
                    column="isBack"
                length="2"
        >

        </property>

    </class>    

</hibernate-mapping>


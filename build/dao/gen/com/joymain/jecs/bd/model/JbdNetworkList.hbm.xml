<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.bd.model.JbdNetworkList"
            table="JBD_NETWORK_LIST"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_BD]]>
        </param>

    </generator>

    </id>

        <property
            name="wyear"
                    column="W_YEAR"
                length="8"
        >

        </property>

        <property
            name="wmonth"
                    column="W_MONTH"
                length="8"
        >

        </property>

        <property
            name="wweek"
                    column="W_WEEK"
                length="8"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
        >

        </property>

        <property
            name="companyCode"
                    column="COMPANY_CODE"
                length="4"
        >

        </property>

        <property
            name="startMonth"
                    column="START_MONTH"
                length="8"
        >

        </property>

        <property
            name="endMonth"
                    column="END_MONTH"
                length="8"
        >

        </property>

        <property
            name="networkMoney"
                    column="NETWORK_MONEY"
                length="22"
        >

        </property>

        <property
            name="balanceMoney"
                    column="BALANCE_MONEY"
                length="22"
        >

        </property>

        <property
            name="deductMoney"
                    column="DEDUCT_MONEY"
                length="22"
        >

        </property>

        <property
            name="calcWeek"
                    column="CALC_WEEK"
                length="8"
        >

        </property>

    </class>    

</hibernate-mapping>


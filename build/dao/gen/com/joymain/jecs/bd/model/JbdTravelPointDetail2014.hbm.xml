    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_BD]]>
        </param>

    </generator>

    </id>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
        >

        </property>

        <property
            name="travelType"
                    column="TRAVEL_TYPE"
                length="1"
        >

        </property>

        <property
            name="usePoint"
                    column="USE_POINT"
                length="22"
        >

        </property>

        <property
            name="createUser"
                    column="CREATE_USER"
                length="20"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

    </class>    

</hibernate-mapping>


<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.bd.model.JbdManuallyAdjustAlgebra"
            table="JBD_MANUALLY_ADJUST_ALGEBRA"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_NBD]]>
        </param>

    </generator>

    </id>

        <property
            name="wweek"
                    column="W_WEEK"
                length="8"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
        >

        </property>

        <property
            name="algebra"
                    column="ALGEBRA"
                length="4"
        >

        </property>

        <property
            name="createNo"
                    column="CREATE_NO"
                length="500"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

    </class>    

</hibernate-mapping>


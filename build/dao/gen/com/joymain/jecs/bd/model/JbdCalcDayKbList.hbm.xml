<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.bd.model.JbdCalcDayKbList"
            table="JBD_CALC_DAY_KB_LIST"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_BD]]>
        </param>

    </generator>

    </id>

        <property
            name="status"
                    column="STATUS"
        >

        </property>

        <property
            name="wweek"
                    column="W_WEEK"
                length="22"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
        >

        </property>

        <property
            name="userName"
                    column="USER_NAME"
                length="200"
        >

        </property>

        <property
            name="kbMoney"
                    column="KB_MONEY"
                length="22"
        >

        </property>

        <property
            name="kbReason"
                    column="KB_REASON"
                length="200"
        >

        </property>

        <property
            name="operationNo"
                    column="OPERATION_NO"
                length="20"
        >

        </property>

        <property
            name="operationDate"
                    column="OPERATION_DATE"
                length="7"
        >

        </property>

    </class>    

</hibernate-mapping>


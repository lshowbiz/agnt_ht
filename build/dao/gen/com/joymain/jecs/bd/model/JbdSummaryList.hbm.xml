<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.bd.model.JbdSummaryList"
            table="JBD_SUMMARY_LIST"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[JD_SEQ]]>
        </param>

    </generator>

    </id>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
        >

        </property>

        <property
            name="cardType"
                    column="CARD_TYPE"
                length="20"
        >

        </property>

        <property
            name="inType"
                    column="IN_TYPE"
                length="22"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="orderType"
                    column="ORDER_TYPE"
                length="2"
        >

        </property>

        <property
            name="oldCheckDate"
                    column="OLD_CHECK_DATE"
                length="7"
        >

        </property>

        <property
            name="newCheckDate"
                    column="NEW_CHECK_DATE"
                length="7"
        >

        </property>

        <property
            name="pvAmt"
                    column="PV_AMT"
                length="22"
        >

        </property>

        <property
            name="oldLinkNo"
                    column="OLD_LINK_NO"
                length="20"
        >

        </property>

        <property
            name="newLinkNo"
                    column="NEW_LINK_NO"
                length="20"
        >

        </property>

        <property
            name="oldRecommendNo"
                    column="OLD_RECOMMEND_NO"
                length="20"
        >

        </property>

        <property
            name="newRecommendNo"
                    column="NEW_RECOMMEND_NO"
                length="20"
        >

        </property>

        <property
            name="newCompanyCode"
                    column="NEW_COMPANY_CODE"
                length="2"
        >

        </property>

        <property
            name="userCreateTime"
                    column="USER_CREATE_TIME"
                length="7"
        >

        </property>

        <property
            name="wweek"
                    column="W_WEEK"
                length="22"
        >

        </property>

        <property
            name="orderNo"
                    column="ORDER_NO"
                length="20"
        >

        </property>

    </class>    

</hibernate-mapping>


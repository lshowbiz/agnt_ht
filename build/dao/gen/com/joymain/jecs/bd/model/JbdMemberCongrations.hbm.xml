<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.joymain.jecs.bd.model.JbdMemberCongrations"
            table="JBD_MEMBER_CONGRATIONS"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.Long"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="sequence">

        <!-- The generator-param tag is deprecated, use the @hibernate.generator and @hibernate.param tags instead -->
        <param
                name="sequence"
        >
            <![CDATA[SEQ_BD]]>
        </param>

    </generator>

    </id>

        <property
            name="yearMonth"
                    column="YEAR_MONTH"
                length="8"
        >

        </property>

        <property
            name="userCode"
                    column="USER_CODE"
                length="20"
        >

        </property>

        <property
            name="starLevel"
                    column="STAR_LEVEL"
                length="2"
        >

        </property>

        <property
            name="status"
                    column="STATUS"
                length="1"
        >

        </property>

        <property
            name="remark"
                    column="REMARK"
                length="4000"
        >

        </property>

        <property
            name="createUser"
                    column="CREATE_USER"
                length="20"
        >

        </property>

        <property
            name="createTime"
                    column="CREATE_TIME"
        >

        </property>

        <property
            name="chineseName"
                    column="CHINESE_NAME"
                length="400"
        >

        </property>

        <property
            name="englishName"
                    column="ENGLISH_NAME"
                length="400"
        >

        </property>

    </class>    

</hibernate-mapping>

